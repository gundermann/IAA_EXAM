<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE tiles-definitions PUBLIC
		"-//Apache Software Foundation//DTD Tiles Configuration 2.0//EN"
		"http://tiles.apache.org/dtds/tiles-config_2_0.dtd">

<tiles-definitions>
	<!-- The main layout. -->
	<definition name="mainLayout" template="/layout/mainLayout.jsp">
		<put-attribute name="title" value="Library" />
		<put-attribute name="header" value="/header.jsp" />
	</definition>



	<!-- The list screen for all cars. -->
	<definition name="carList" extends="mainLayout">
		<put-attribute name="content" value="/carList.jsp" />
	</definition>

	<!-- The form for a single car. -->
	<definition name="carForm" extends="mainLayout">
		<put-attribute name="content" value="/carForm.jsp" />
	</definition>




	


	<!-- INVENTORYMANAGEMENT -->
	<!-- The main layout for master data management. -->
	<definition name="inventoryManagementLayout" template="/layout/inventoryManagementLayout.jsp">
		<put-attribute name="title" value="InventoryManagement" />
		<put-attribute name="inventoryManagementHeader"
			value="/InventoryManagement/inventoryManagementHeader.jsp" />
		<put-attribute name="header" value="/header.jsp" />
	</definition>
	<!-- The menu of the inventory management -->
	<definition name="inventoryManagement" extends="inventoryManagementLayout">
		<put-attribute name="content"
			value="/InventoryManagement/inventoryManagement.jsp" />
	</definition>
	<!-- form for publications -->
	<definition name="publicationForm" extends="inventoryManagementLayout">
		<put-attribute name="content"
			value="/InventoryManagement/publicationForm.jsp" />
	</definition>
	<!-- form for common publication search -->
	<definition name="publicationSearchForm" extends="inventoryManagementLayout">
		<put-attribute name="content"
			value="/InventoryManagement/publicationSearchForm.jsp" />
	</definition>


	<!-- MASTERDATAMANAGEMENT -->
	<!-- The main layout for master data management. -->
	<definition name="masterDataManagementLayout" template="/layout/masterDataManagementLayout.jsp">
		<put-attribute name="title" value="MasterDataManagement" />
		<put-attribute name="masterDataManagementHeader"
			value="/MasterDataManagement/masterDataManagementHeader.jsp" />
		<put-attribute name="header" value="/header.jsp" />
	</definition>
	<!-- Menü für die Stammdatenverwaltung -->
	<definition name="masterDataManagement" extends="masterDataManagementLayout">
		<put-attribute name="content"
			value="/MasterDataManagement/masterDataManagement.jsp" />
	</definition>
	<!-- List of publicationstypes -->
	<definition name="publicationTypeList" extends="masterDataManagementLayout">
		<put-attribute name="masterDataManagementHeader"
			value="/MasterDataManagement/publicationTypeHeader.jsp" />
		<put-attribute name="content"
			value="/MasterDataManagement/publicationTypeList.jsp" />
	</definition>
	<!-- Formular for new publicationtype -->
	<definition name="publicationTypeForm" extends="masterDataManagementLayout">
		<put-attribute name="masterDataManagementHeader"
			value="/MasterDataManagement/publicationTypeHeader.jsp" />
		<put-attribute name="content"
			value="/MasterDataManagement/publicationTypeForm.jsp" />
	</definition>
	<!-- Liste der Schlüsselwörter -->
	<definition name="keywordList" extends="masterDataManagementLayout">
		<put-attribute name="masterDataManagementHeader"
			value="/MasterDataManagement/keywordHeader.jsp" />
		<put-attribute name="content"
			value="/MasterDataManagement/keywordList.jsp" />
	</definition>
	<!-- Formular für neues Schlüsselwort -->
	<definition name="keywordForm" extends="masterDataManagementLayout">
		<put-attribute name="masterDataManagementHeader"
			value="/MasterDataManagement/keywordHeader.jsp" />
		<put-attribute name="content"
			value="/MasterDataManagement/keywordForm.jsp" />
	</definition>
	<!-- Liste der Ausleiher -->
	<definition name="lenderList" extends="masterDataManagementLayout">
		<put-attribute name="masterDataManagementHeader"
			value="/MasterDataManagement/lenderHeader.jsp" />
		<put-attribute name="content"
			value="/MasterDataManagement/lenderList.jsp" />
	</definition>
	<!-- Formular for lender -->
	<definition name="lenderForm" extends="masterDataManagementLayout">
		<put-attribute name="masterDataManagementHeader"
			value="/MasterDataManagement/lenderHeader.jsp" />
		<put-attribute name="content"
			value="/MasterDataManagement/lenderForm.jsp" />
	</definition>
	<!-- list of authors -->
	<definition name="authorList" extends="masterDataManagementLayout">
		<put-attribute name="masterDataManagementHeader"
			value="/MasterDataManagement/authorHeader.jsp" />
		<put-attribute name="content"
			value="/MasterDataManagement/authorList.jsp" />
	</definition>
	<!-- Form for author -->
	<definition name="authorForm" extends="masterDataManagementLayout">
		<put-attribute name="masterDataManagementHeader"
			value="/MasterDataManagement/authorHeader.jsp" />
		<put-attribute name="content"
			value="/MasterDataManagement/authorForm.jsp" />
	</definition>
	
	<!-- ADMONITIONMANAGEMENT -->
	<!-- The main layout for admonition management. -->
	<definition name="admontionManagementLayout" template="/layout/admonitionManagementLayout.jsp">
		<put-attribute name="title" value="AdmonitionManagement" />
		<put-attribute name="admonitionManagementHeader"
			value="/AdmonitionManagement/admonitionManagementHeader.jsp" />
		<put-attribute name="header" value="/header.jsp" />
	</definition>
	<!-- the main menu for admonition management -->
	<definition name="admonitionManagement" extends="admontionManagementLayout">
		<put-attribute name="content" value="/AdmonitionManagement/admonitionManagement.jsp" />
	</definition>
	<!-- list of delayed lending -->
	<definition name="delayedLendingList" extends="admontionManagementLayout">
		<put-attribute name="content" value="/AdmonitionManagement/delayedLendingList.jsp" />
	</definition>
	
</tiles-definitions>
